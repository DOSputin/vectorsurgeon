							label="Amount"
							parameter="KeyboardModAmount"
							min="-99"
							max="99"	
							majortick="10"	
						/>
					</horizontalcenter>
					<verticalbox>
						<checkbox
							label="Filter"
							parameter="KeyboardFilterCutoffMod"
						/>
						<checkbox
							label="Mix AC"
							parameter="KeyboardMixACMod"
						/>
						<checkbox
							label="Mix BD"
							parameter="KeyboardMixBDMod"
						/>
						<checkbox
							label="Pan"
							parameter="KeyboardPanMod"
						/>
					</verticalbox>
				</verticalbox>
				<verticalbox
					name="Filter Env"
				>
					<horizontalcenter>
						<slider
							label="Amount"
							parameter="FilterEnvModAmount"
							min="-99"
							max="99"
							majortick="10"
						/>
					</horizontalcenter>
					<verticalbox>
						<checkbox
							label="Freq ABCD"
							parameter="FilterEnvFreqMod"
						/>
						<checkbox
							label="Pan"
							parameter="FilterEnvPanMod"
						/>
					</verticalbox>
				</verticalbox>
			
				<horizontalbox
					name="Mod Wheel"
					>
					<verticalcenter>
						<verticalbox>
							<checkbox
								label="LFO1 Am"
								parameter="ModWheelLFO1AmountMod"
							/>
							<checkbox
								label="LFO2 Am"
								parameter="ModWheelLFO2AmountMod"
							/>
							<checkbox
								label="ChoRate"
								parameter="ModWheelChorusDepthMod"
							/>
						</verticalbox>
					</verticalcenter>
				</horizontalbox>
				
			</horizontalcenter>
		</horizontalbox>
	</page>
	
	
	<!-- voice control page -->
	
	<page
		name="Voice Control"
		>
		<verticalbox>
			<horizontalcenter>
				<horizontalbox
					name="Arpeggiator"
					>
					<slider
						label="Rate"
						parameter="ArpRate"
						min="0"
						max="99"
						majortick="10"
					/>
					<verticalcenter>
						<verticalbox>
							<popup
								label="Mode"
								parameter="ArpMode"
								value="Normal,Auto-Latch,Extend,Value3"
							/>
							<popup
								label="Scan"
								parameter="ArpScan"
								value="Up,Down,Up/Down,Assign,Reverse,Random,Value6,Value7"
							/>
							<popup
								label="Octaves"
								parameter="ArpOctaves"
								value="1,2,3,4"
							/>
							<popup
								label="Repeats"
								parameter="ArpRepeats"
								value="1,2,3,4"
							/>
						</verticalbox>
					</verticalcenter>
					<verticalcenter>
						<verticalbox>
							<popup
								label="Split"
								parameter="ArpSplit"
								value="Off,Left,Right,Dual"
							/>
							<popup
								label="Voicing"
								parameter="ArpVoicing"
								value="Mono,Poly"
							/>
							<checkbox
								label="Velocity"
								parameter="ArpVelocity"
							/>
							<checkbox
								label="Layer"
								parameter="ArpLayer"
							/>
							<checkbox
								label="Rest"
								parameter="ArpRest"
							/>
						</verticalbox>
					</verticalcenter>
				</horizontalbox>
				<horizontalbox
					name="Keyboard Mode"
					>
					<verticalcenter>
						<verticalbox>
							<popup
								label="Mode"
								parameter="KeyboardMode"
								value="Single,Split,Double"
							/>
						</verticalbox>
					</verticalcenter>
					<slider
						label="Split Point"
						parameter="SplitPoint"
						min="0"
						max="127"
						majortick="10"
					/>
					<slider
						label="Link Program"
						parameter="LinkProgram"
						min="0"
						max="99"
						majortick="10"
					/>
					<slider
						label="Double Detune"
						parameter="DoubleModeDetune"
						min="0"
						max="31"
						majortick="4"
					/>
					<slider
						label="Double Delay"
						parameter="DoubleModeDelay"
						min="0"
						max="127"
						majortick="10"
					/>
				</horizontalbox>
				<horizontalbox
					name="Voice Control"
					>
					<slider
						label="Unison/Detune"
						parameter="UnisonDetune"
						min="0"
						max="7"
						majortick="1"
					/>
					<slider
						label="Glide"
						parameter="Glide"
						min="0"
						max="99"
						majortick="10"
					/>
					<slider
						label="Volume"
						parameter="ProgramVolume"
						min="0"
						max="99"
						majortick="10"
					/>
				</horizontalbox>
				<verticalbox
					name="Chorus"
					>
					<horizontalcenter>
						<!-- there's no way of handling the Prophet's chorus bits separately -->
						<popup
							label="Chorus"
							parameter="ChorusRightLeft"
							value="Off,Right,Left,Left+Right"
						/>						
					</horizontalcenter>
					<horizontalbox>
						<slider
							label="Rate"
							parameter="ChorusRate"
							min="0"
							max="99"
							majortick="10"
						/>
						<slider
							label="Depth"
							parameter="ChorusDepth"
							min="0"
							max="99"
							majortick="10"
						/>
					</horizontalbox>
				</verticalbox>
				<horizontalbox>
					<title
						label="Patch Name"
					/>
					<textfield
						parameter="$PatchName"
						length="8"
					/>
				</horizontalbox>
			</horizontalcenter>
		</verticalbox>
	</page>
	
</machine>


